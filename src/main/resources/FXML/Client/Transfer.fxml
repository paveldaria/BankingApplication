<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" styleClass="main_container" stylesheets="@../../CSS/TransferMoney.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.mazebank.Controllers.Client.TransferController">
      <VBox prefHeight="600.0" prefWidth="253.0" styleClass="Vbox" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="0.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="Vbox_Title" text="Transfer funds">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="Vbox_Text" text="Search by username">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Text>
            <TextField fx:id="usernameField" styleClass="Vbox_TextField" />
      <Button fx:id="searchButton" mnemonicParsing="false" prefHeight="32.0" prefWidth="289.0" text="Search">
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin></Button>
      <Label fx:id="messageLabel_search" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="Vbox_Text" text="Amount">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin></Text>
            <TextField fx:id="amountField" styleClass="Vbox_TextField" />
            <Button fx:id="transferButton" mnemonicParsing="false" prefHeight="13.0" prefWidth="253.0" text="Transfer">
               <VBox.margin>
                  <Insets top="3.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="messageLabel_transfer" />
      </VBox>
</AnchorPane>
